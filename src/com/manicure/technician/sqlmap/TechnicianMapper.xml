  <select id="select3GoodTechs" resultMap="3GoodTechResultMap">
 select te.id,te.name,te.header_pic,te.job_title,sum(oc.evaluation) evaluation,
avg(oc.stars) stars,avg(oc.profnal_score) profnal_score,
